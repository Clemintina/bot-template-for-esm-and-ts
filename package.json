{
  "name": "template-discord-bot",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "testModule": "bun build && node --experimental-specifier-resolution=node dist/main.js",
    "build": "tsc",
    "start": "node dist/main.js",
    "format": "prettier src --write",
    "lint": "eslint . --ext .ts",
    "lint-and-format": "prettier src --write && eslint .eslintrc --ext .ts",
    "upgrade": "pnpm up -L"
  },
  "dependencies": {
    "@discordjs/rest": "^2.0.1",
    "bufferutil": "^4.0.8",
    "discord-api-types": "^0.37.63",
    "discord.js": "^14.13.0",
    "got": "^13.0.0",
    "i18next": "^23.7.3",
    "pino": "^8.16.1",
    "pino-pretty": "^10.2.3",
    "utf-8-validate": "^6.0.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "bun-types": "^1.0.11",
    "eslint": "^8.53.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": "20"
  }
}
